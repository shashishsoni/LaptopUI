{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "uploadConcurrency": 5,
        "maxConcurrentUploadParts": 5
      }
    }
  ],
  "routes": [
    {
      "src": "/videos/(.*)",
      "dest": "/public/videos/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "video/mp4"
      }
    }
  ],
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  }
} 