{
  "name": "laptopui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/init-prisma.js",
    "vercel-build": "eslint . --max-warnings 0 && prisma generate --schema=./src/server/prisma/schema.prisma && next build",
    "dev": "npm run postinstall && next dev",
    "server": "cd src/server && ts-node src/server.ts",
    "dev:server": "cd src/server && nodemon src/server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "npm run postinstall && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=./src/server/prisma/schema.prisma || exit 0",
    "prisma:push": "prisma db push --schema=./src/server/prisma/schema.prisma"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.3.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.6.0",
    "framer-motion": "^12.4.2",
    "gsap": "^3.12.7",
    "jsonwebtoken": "^9.0.2",
    "next": "15.1.6",
    "prisma": "^6.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.15.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "stripe": "^17.6.0",
    "three": "^0.173.0",
    "three.regular.helvetiker": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/gsap": "^1.20.2",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.173.0",
    "@types/webpack": "^5.28.5",
    "concurrently": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "nodemon": "^3.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5",
    "webpack": "^5.97.1"
  }
}
