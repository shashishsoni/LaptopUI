{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "laptopui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "server": "cd src/server && ts-node src/server.ts",
    "dev:server": "cd src/server && nodemon src/server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "next build",
    "clean": "node scripts/clean-build.js",
    "copy-assets": "node scripts/copy-videos.js",
    "vercel-build": "prisma generate --schema=./src/server/prisma/schema.prisma && prisma db push --schema=./src/server/prisma/schema.prisma && next build && npm run copy-assets",
    "postinstall": "prisma generate --schema=./src/server/prisma/schema.prisma && node scripts/post-install.js",
    "start": "next start",
    "lint": "next lint",
    "build:all": "node scripts/build.js",
    "install:all": "npm install && cd src/server && npm install",
    "install:clean": "rimraf node_modules && rimraf src/server/node_modules && npm run install:all"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^5.22.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.6.0",
    "axios": "^1.7.9",
    "critters": "^0.0.20",
    "framer-motion": "^12.4.2",
    "gsap": "^3.12.7",
    "jsonwebtoken": "^9.0.2",
    "next": "15.1.6",
    "prisma": "^5.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.15.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "stripe": "^17.6.0",
    "three": "^0.173.0",
    "three.regular.helvetiker": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/gsap": "^1.20.2",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.173.0",
    "@types/webpack": "^5.28.5",
    "concurrently": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "file-loader": "^6.2.0",
    "nodemon": "^3.0.0",
    "postcss": "^8",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5",
    "webpack": "^5.97.1"
  }
}
